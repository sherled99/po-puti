{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "installCommand": "npm install",
  "buildCommand": "CI=false npm run build",
  "outputDirectory": "build",
  "rewrites": [
    {
      "source": "/backend/(.*)",
      "destination": "/backend/$1"
    },
    {
      "source": "/((?!backend).*)",
      "destination": "/index.html"
    }
  ]
}

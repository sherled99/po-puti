{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "installCommand": "npm install",
  "buildCommand": "CI=false npm run build",
  "outputDirectory": "build",
  "rewrites": [
    {
      "source": "/api/_proxy/(.*)",
      "destination": "/api/_proxy/$1"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ]
}
